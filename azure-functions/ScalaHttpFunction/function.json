{
  "scriptFile": "../seat-stalker.jar",
  "entryPoint": "fi.jpaju.azurefunction.ScalaHttpFunction.run",
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "GET",
        "POST"
      ],
      "authLevel": "ANONYMOUS"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
